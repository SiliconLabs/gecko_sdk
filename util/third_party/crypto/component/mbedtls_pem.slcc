id: mbedtls_pem
package: platform
label: Mbed TLS Support for PEM Parsing
description: >-
  This component provides support for reading PEM formatted keys and certificates in Mbed TLS.
category: Platform|Security
quality: production
source:
  - path: util/third_party/crypto/mbedtls/library/pem.c
include:
  - path: util/third_party/crypto/mbedtls/include
    file_list:
      - path: mbedtls/pem.h
      - path: mbedtls/des.h
      - path: mbedtls/md5.h
provides:
  - name: mbedtls_pem
requires:
  - name: mbedtls_cipher
  - name: mbedtls_base64
  - name: mbedtls_core
template_contribution:
  - name: mbedtls_config
    value: MBEDTLS_PEM_PARSE_C
  - name: mbedtls_config
    value: MBEDTLS_PEM_WRITE_C
