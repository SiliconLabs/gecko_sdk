    value: MBEDTLS_PSA_ACCEL_ALG_SHA_1
    condition: [device_series_1]
  - name: psa_crypto_config
    value: MBEDTLS_PSA_ACCEL_ALG_SHA_1
    condition: [device_series_2]
  - name: psa_crypto_config
    value: MBEDTLS_PSA_ACCEL_ALG_SHA_224
    condition: [device_series_1]
  - name: psa_crypto_config
