requires:
  - name: device
  - name: psa_crypto_common
  - name: emlib_crypto
    condition: [device_has_crypto]
  - name: cryptoacc_library
    condition: [device_has_cryptoacc]
  - name: se_manager
    condition: [device_series_2]
provides:
  - name: psa_driver
template_contribution:
